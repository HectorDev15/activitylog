{
    "name": "hectordev15/activitylog",
    "description": "A very simple activity logger to monitor the users of your website or application",
    "homepage": "",
    "keywords":
    [
        "spatie",
        "log",
        "user",
        "activity",
        "laravel"
    ],
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/config": "5.5.x||5.6.x||5.7.x",
        "illuminate/database": "5.5.x||5.6.x||5.7.x",
        "illuminate/support": "5.5.x||5.6.x||5.7.x",
        "spatie/string": "^2.1",
        "jenssegers/mongodb": "^3.2"

    },
    "require-dev": {
        "phpunit/phpunit": "~6.2",
        "orchestra/testbench": "~3.5.0",
        "orchestra/database": "~3.5.0"
    },
    "autoload": {
        "psr-4": {
            "RifkyEkayama\\Activitylog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RifkyEkayama\\Activitylog\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "RifkyEkayama\\Activitylog\\ActivitylogServiceProvider"
            ]
        }
    }
}
